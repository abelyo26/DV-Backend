{
  "name": "express-rest",
  "version": "0.0.1",
  "description": "Boilerplate for node, express, mongoose REST API",
  "main": "server.js",
  "author": "abdulhamidOumer",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon src/index.js --exec babel-node -e js",
    "start:prod": "yarn build && yarn build-html && NODE_ENV=production node build/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s && npx --yes copyfiles -u 1 './src/**/*.html' ./build",
    "build": "yarn clean && yarn build-babel",
    "build-html": "cp src/utils/templates/*.html build/utils/templates",
    "lint": "eslint . --color -f table",
    "test": "NODE_ENV=test mocha __tests__ --require @babel/register --recursive --exit --timeout 10000",
    "coverage": "nyc yarn test",
    "lint:inspect": "eslint --inspect-config",
    "generate": "babel-node node_modules/.bin/plop --plopfile internals/generators/plopfile.js",
    "prepare": "husky install",
    "run-script": "NODE_ENV=development babel-node src/scripts",
    "build-seeder": "rm -rf ./build/seeder && babel -d ./build/seeder ./src/scripts/seed -s",
    "seed-admin": "node ./build/scripts/seed/adminSeeder.js"
  },
  "engines": {
    "node": ">=18"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@inquirer/prompts": "^5.1.0",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "commander": "^12.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "flat": "^4.1.1",
    "googleapis": "^135.1.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "joi": "^17.13.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.1.0",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf2json": "^4.0.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/node": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/register": "^7.23.7",
    "@babel/runtime": "^7.24.5",
    "@eslint/js": "^9.5.0",
    "chai": "4.4.1",
    "chai-http": "^4.4.0",
    "eslint": "^9.1.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.4.0",
    "eslint-plugin-mocha": "^10.4.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-watch": "^8.0.0",
    "globals": "^15.7.0",
    "husky": "^9.0.11",
    "inquirer": "^9.2.20",
    "inquirer-directory": "^2.2.0",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "plop": "^4.0.1",
    "prettier": "^3.2.5",
    "prettify": "^0.1.7",
    "shelljs": "^0.8.5"
  }
}
